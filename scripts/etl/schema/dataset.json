{
  "title": "AMR ETL datasets",
  "type": "array",
  "minItems": 1,
  "items": {
    "type": "object",
    "required": [
      "name",
      "path"
    ],
    "properties": {
      "name": {
        "type": "string",
        "description": "dataset name"
      },
      "path": {
        "type": "string",
        "description": "dataset path"
      },
      "filter": {
        "type": "string",
        "description":"Path of list to values used to filter dataset by"
      },
      "filter_column": {
        "type": "string",
        "description": "name of column to filter by"
      }
    }
  }
}