<div class="subheader">
  <div class="left-wrapper">
    <a href="/">
      <svg width="32" height="32" viewBox="0 0 32 32" class="home-icon" aria-hidden="true">
        <use href="{{ '/assets/images/icons/ensembl-icons/icon_home.svg' | htmlBaseUrl }}"></use>
      </svg>
      <span class="visually-hidden">
        Home page
      </span>
    </a>

    <div class="title-wrapper">
      <span class="portal-title">
        Anti-microbial resistance portal
      </span>

      <span class="page-title">
        {{ header_title }}
      </span>
    </div>
  </div>

  <div class="right-wrapper">
    <amr-header-release>
      <span class="data-release">
        <span class="data-release-label">
          Latest data release
        </span>
        <span class="data-release-date">
          yyyy-mm
        </span>
      </span>
    </amr-header-release>

    <a href="/about">
      <svg width="32" height="32" viewBox="0 0 32 32" class="circle-icon">
        <use href="{{ '/assets/images/icons/ensembl-icons/icon_question_circle.svg' | htmlBaseUrl }}"></use>
      </svg>
      <span class="visually-hidden">
        Help page
      </span>
    </a>
  </div>
</div>